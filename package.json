{
  "name": "@duplojs/to",
  "version": "0.0.0",
  "author": "mathcovax",
  "description": "",
  "repository": "https://github.com/duplojs/duplojs-to",
  "license": "ISC",
  "keywords": [],
  "types": "./types/scripts/index.d.ts",
  "exports": {
    ".": {
      "types": "./types/scripts/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./plugin": {
      "types": "./plugin.d.ts",
      "import": "./dist/plugin.mjs",
      "require": "./dist/plugin.cjs"
    }
  },
  "files": [
    "dist",
    "types"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rollup --config && npx tsc -p tsconfig.build.json",
    "test:e2e": "node --require sucrase/register test",
    "test:types": "node --require sucrase/register test/plugin/route.ts --generate-types && tsc"
  },
  "devDependencies": {
    "@duplojs/worker-testing": "^0.1.5",
    "@rollup/plugin-json": "^6.1.0",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "rollup": "^3.26.2",
    "rollup-plugin-esbuild": "^6.1.1",
    "sucrase": "^3.34.0",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "@duplojs/duplojs": ">=1.2.14 <2.0.0",
    "@duplojs/what-was-sent": ">=1.2.1 <2.0.0",
    "zod": "^3.22.4",
    "zod-to-ts": "^1.2.0"
  }
}
